    "version": "0.2.0",
    "configurations": [
        {
            "name": "neurossistant",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "--dart-define", "ENV_ENCRYPTION_KEY=your_encryption_key_here",
                "--dart-define", "ENV_IV_KEY=your_iv_key_here",
            ],
            "console": "debugConsole",
        },
        {
            "name": "neurossistant (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define", "ENV_ENCRYPTION_KEY=your_encryption_key_here",
                "--dart-define", "ENV_IV_KEY=your_iv_key_here",
            ],
        },
        {
            "name": "neurossistant (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--dart-define", "ENV_ENCRYPTION_KEY=your_encryption_key_here",
                "--dart-define", "ENV_IV_KEY=your_iv_key_here",
            ],
        }
    ]
}